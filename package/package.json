{
  "name": "@chartiq/chartiq",
  "version": "6.3.0",
  "homepage": "https://github.com/ChartIQ/stx#readme",
  "private": true,
  "main": "dist/chartiq.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChartIQ/stx.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "webpack -p && webpack --env=lite --mode=production",
    "build:react": "webpack --config=webpack.react.js",
    "concat-lib": "sh concat-lib.sh",
    "concat-lib-node": "node quick-build.js",
    "docs:build": "gulp docs:build",
    "docs:watch": "gulp docs:watch",
    "eslint": "eslint --config eslint.yaml htdocs/default && eslint --config eslint-spec.yaml spec gulpfile.js webpack.config.js",
    "eslint:fix": "eslint --fix --config eslint.yaml htdocs/default && eslint --config eslint-spec.yaml --fix spec gulpfile.js webpack.config.js",
    "htmlhint": "htmlhint htdocs/default/*.html",
    "jshint": "jshint --reporter=node_modules/jshint-stylish htdocs/default/js htdocs/default/plugins && jshint --reporter=node_modules/jshint-stylish --extract=auto htdocs/default/*.html",
    "karma": "karma start",
    "karma:watch": "karma start --auto-watch",
    "release": "release-it",
    "postinstall": "npm run concat-lib-node",
    "postbuild": "gulp build:prep-templates && gulp build:copy-files",
    "scss": "npm run scss:ciq && npm run scss:tfc && npm run scss:cryptoiq && npm run scss:scriptiq",
    "scss:ciq": "node-sass -o htdocs/default/css --source-map true htdocs/default/sass/chartiq.scss",
    "scss:tfc": "node-sass -o htdocs/default/plugins/tfc --source-map true htdocs/default/plugins/tfc/tfc.scss",
    "scss:cryptoiq": "node-sass -o htdocs/default/plugins/cryptoiq --source-map true htdocs/default/plugins/cryptoiq/cryptoiq.scss",
    "scss:scriptiq": "node-sass -o htdocs/default/plugins/scriptiq --source-map true htdocs/default/plugins/scriptiq/scriptiq-editor.scss",
    "selenium": "concurrently \"npm run watch\" \"selenium-standalone install && selenium-standalone start\"",
    "stylelint": "stylelint \"htdocs/default/sass/*.scss\" --config stylelint.yaml",
    "test": "npm run eslint && npm run umd && npm run stylelint && npm run karma",
    "test:ci": "npm run test && npm run wdio && npm run docs:build",
    "umd": "cross-env GITHUB_TOKEN=bc79b6b01b11a4aa291d59460bdfd5e6abacbfd6 node verifyUMD.js",
    "watch": "webpack-dev-server --inline --hot",
    "watch:react": "webpack-dev-server --inline --config=webpack.react.js --hot",
    "wdio": "wdio",
    "wdio:debug": "DEBUG=true wdio",
    "wdio:spec": "wdio --spec",
    "wdio:debug:spec": "DEBUG=true wdio --spec",
    "wdio:watch": "wdio --watch",
    "wdio-perf": "wdio wdio-performance.conf.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "ajv": ">=5.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.0.0",
    "concurrently": "^3.4.0",
    "cross-env": "^5.1.0",
    "eslint": "^3.16.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-unicorn": "^2.0.1",
    "glob": "^7.1.1",
    "gulp": "^4.0.0",
    "gulp-changed": "^2.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-replace": "^0.6.1",
    "gulp-sync": "^0.1.4",
    "htmlhint": "^0.10.1",
    "intl": "^1.2.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jsdoc": "^3.4.3",
    "jshint": "^2.9.7",
    "jshint-stylish": "^2.2.1",
    "karma": "^3.1.4",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-plugins": "^0.9.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "4.0.0-rc.5",
    "mocha": "^5.2.0",
    "moment": "^2.19.1",
    "natives": "^1.1.6",
    "node-sass": "^4.9.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "release-it": "^9.7.0",
    "sanitize-html": "^1.14.1",
    "selenium-standalone": "^6.15.3",
    "sinon": "~3.2.1",
    "stylelint": "^7.9.0",
    "stylelint-scss": "^1.4.3",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-selenium-standalone-service": "^0.0.10",
    "wdio-spec-reporter": "^0.1.4",
    "wdio-static-server-service": "^1.0.0",
    "webdriverio": "^4.13.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
